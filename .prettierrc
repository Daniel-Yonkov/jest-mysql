{
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"htmlWhitespaceSensitivity": "css",
	"bracketSameLine": false,
	"parser": "babel",
	"printWidth": 80,
	"proseWrap": "preserve",
	"requirePragma": false,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "none",
	"overrides": [
        {
          "files": [".prettierrc","*.json"],
          "options": { "parser": "json" }
        }
      ]
}
